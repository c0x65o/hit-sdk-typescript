{
  "name": "@hit/sdk",
  "version": "1.2.17",
  "description": "Strongly-typed TypeScript client libraries for Hit platform microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "default": "./dist/server.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js",
      "default": "./dist/next.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "default": "./dist/ui/index.js"
    },
    "./ui/context": {
      "types": "./dist/ui/context.d.ts",
      "import": "./dist/ui/context.js",
      "default": "./dist/ui/context.js"
    },
    "./ui/hooks": {
      "types": "./dist/ui/hooks.d.ts",
      "import": "./dist/ui/hooks.js",
      "default": "./dist/ui/hooks.js"
    },
    "./ui/types": {
      "types": "./dist/ui/types.d.ts",
      "import": "./dist/ui/types.js",
      "default": "./dist/ui/types.js"
    }
  },
  "scripts": {
    "build": "tsc && node scripts/post-build.js",
    "test": "echo \"No tests yet\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hit",
    "sdk",
    "microservices",
    "typescript"
  ],
  "author": "Hitcents, Inc.",
  "license": "PROPRIETARY",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^19.0.0",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "zod": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  }
}

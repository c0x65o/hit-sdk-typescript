{
  "name": "@hit/sdk",
  "version": "1.2.22",
  "description": "Strongly-typed TypeScript client libraries for Hit platform microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "default": "./dist/server.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js",
      "default": "./dist/next.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "default": "./dist/ui/index.js"
    },
    "./ui/context": {
      "types": "./dist/ui/context.d.ts",
      "import": "./dist/ui/context.js",
      "default": "./dist/ui/context.js"
    },
    "./ui/hooks": {
      "types": "./dist/ui/hooks.d.ts",
      "import": "./dist/ui/hooks.js",
      "default": "./dist/ui/hooks.js"
    },
    "./ui/types": {
      "types": "./dist/ui/types.d.ts",
      "import": "./dist/ui/types.js",
      "default": "./dist/ui/types.js"
    },
    "./date-utils": {
      "types": "./dist/date-utils.d.ts",
      "import": "./dist/date-utils.js",
      "default": "./dist/date-utils.js"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js",
      "default": "./dist/auth.js"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.js",
      "default": "./dist/events.js"
    },
    "./provider": {
      "types": "./dist/provider.d.ts",
      "import": "./dist/provider.js",
      "default": "./dist/provider.js"
    }
  },
  "scripts": {
    "build": "tsc && node scripts/post-build.js",
    "test": "echo \"No tests yet\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hit",
    "sdk",
    "microservices",
    "typescript"
  ],
  "author": "Hitcents, Inc.",
  "license": "PROPRIETARY",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^19.0.0",
    "react": "19.2.3",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "react": "19.2.3",
    "zod": "^4.3.2"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  }
}
